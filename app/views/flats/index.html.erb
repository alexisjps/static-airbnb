<div class="container">
    <h1>Hello World</h1>
    <% @flats.each do |flat| %>
        <div class="card">
        <div class="card-header">
            <%= flat.pricing %>
        </div>
        <div class="card-body">
            <h5 class="card-title"><%= flat.name %></h5>
            <p class="card-text"><%= flat.address %></p>
            <%= link_to "See the appartment", flats_path(flat.id) %>
        </div>
        </div>
    <% end %>
</div>